{
  "name": "code-tool/jaeger-client-symfony-bridge",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Jaeger\\Symfony\\": "src/"
    }
  },
  "require": {
    "php": ">=7.1",
    "symfony/event-dispatcher": "~2 || ~3 || ~4",
    "symfony/console": "~2 || ~3 || ~4",
    "symfony/config": "~2 || ~3 || ~4",
    "symfony/http-kernel": "~2 || ~3 || ~4",
    "symfony/dependency-injection": "~2 || ~3 || ~4",
    "code-tool/jaeger-client-php": "~2.7"
  },
  "require-dev": {
    "php-ds/php-ds": "@stable",
    "phpunit/phpunit": "@stable"
  },
  "suggest": {
    "ext-ds": "Efficient data structures"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
